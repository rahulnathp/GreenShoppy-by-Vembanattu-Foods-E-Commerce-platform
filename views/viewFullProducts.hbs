<section class="sections">
    <div class="row">

      <div class=" col-md-10">
        <div class="products">
          <section style="background-color: #eee;">
            <div class="container py-5">
              <div class="row">
                {{#each stores}}

                <div class="col-md-4">
                  <a href="/userlogin/productView/{{this._id}}?_method=get">
                    <div class="cards">
                      <div class="card">

                        <img src="/category-image/{{this.Front_image}}" class="card-img-top justify-content-center"
                          style="height:250px ;width:250px;" alt="" />
                        <div class="card-body">
                          <div class="d-flex justify-content-between">
                            <p class="small"><a href="#!" class="text-muted">{{Category}}</a></p>
                            <p class="small text-danger fs-4"><s>{{this.Maximum_retail_price}}</s></p>
                          </div>

                          <div class="d-flex justify-content-between mb-3">

                            <h5 class="mb-0 text-success">{{this.Brand_name}}</h5>

                            <h5 class="text-dark mb-0 fs-4">â‚¹{{this.Selling_Price}}</h5>
                          </div>
                          <h5 class="mb-0 text-dark">{{this.Product_title}}</h5>

                          <div class="d-flex justify-content-between mb-2">
                            <p class="text-muted mb-0">{{this.Product_description}} </p>

                          </div>



                        </div>
                      </div>
                    </div>


                </div>
                </a>
                {{/each}}
              </div>



            </div>
        </div>
      </div>
    </div>

  </section>